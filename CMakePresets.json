{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 22
  },
  "configurePresets": [
    {
      "name": "default",
      "hidden": true,
      "generator": "Ninja",
      "binaryDir": "build/${presetName}",
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": true,
        "FETCH_CONENT_SOURCE_DIR_FREERTOS_KERNEL": "dependencies"
      }
    },
    {
      "name": "posix",
      "inherits": "default",
      "cacheVariables": {
        "CONFIG_POSIX_GCC": true,
        "FREERTOS_PORT": "GCC_POSIX"
      }
    },
    {
      "name": "mingw",
      "inherits": "default",
      "cacheVariables": {
        "CONFIG_PIC32MZ": true,
        "FREERTOS_PORT": "MSVC_MINGW"
      }
    },
    {
      "name": "pic32mz",
      "inherits": "default",
      "cacheVariables": {
        "CONFIG_PIC32MZ": true,
        "FREERTOS_PORT": "MPLAB_PIC32MZ"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "default",
      "hidden": true,
      "configurePreset": "default"
    },
    {
      "name": "posix",
      "configurePreset": "posix",
      "cleanFirst": true,
      "jobs": 4
    },
    {
      "name": "mingw",
      "configurePreset": "mingw",
      "cleanFirst": true,
      "jobs": 4
    },
    {
      "name": "pic32mz",
      "configurePreset": "pic32mz",
      "cleanFirst": true,
      "jobs": 4
    }
  ]
}
