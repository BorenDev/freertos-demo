{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 22
  },
  "configurePresets": [
    {
      "name": "base",
      "hidden": true,
      "generator": "Ninja",
      "binaryDir": "build/${presetName}",
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": true
      }
    },
    {
      "name": "local_deps",
      "hidden": true,
      "inherits": "base",
      "cacheVariables": {
        "FETCHCONTENT_SOURCE_DIR_FREERTOS_KERNEL": "dependencies/FreeRTOS-Kernel/"
      }
    },
    {
      "name": "default",
      "hidden": true,
      "inherits": "local_deps"
    },
    {
      "name": "demo_posix",
      "inherits": "default",
      "cacheVariables": {
        "CONFIG_DEMO_POSIX": true,
        "FREERTOS_PORT": "GCC_POSIX",
        "CMAKE_BUILD_TYPE": "Debug",
        "FREERTOS_HEAP": "3"
      }
    },
    {
      "name": "demo_mingw",
      "inherits": "default",
      "cacheVariables": {
        "CONFIG_DEMO_MINGW": true,
        "FREERTOS_PORT": "MSVC_MINGW",
        "CMAKE_BUILD_TYPE": "Debug",
        "FREERTOS_HEAP": "5"
      }
    },
    {
      "name": "demo_pic32mz",
      "inherits": "default",
      "generator": "Unix Makefiles",
      "cacheVariables": {
        "CONFIG_DEMO_PIC32MZ": true,
        "FREERTOS_PORT": "MPLAB_PIC32MZ",
        "CMAKE_BUILD_TYPE": "Debug",
        "XC32_VERSION": "4.35",
        "DFP": "PIC32MZ-EF_DFP",
        "DFP_VERSION": "1.4.168",
        "PIC": "32MZ2048EFM144",
        "MPLABX_VERSION": "6.20",
        "FREERTOS_HEAP": "4"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "default",
      "hidden": true,
      "configurePreset": "default",
      "cleanFirst": false,
      "jobs": 12
    },
    {
      "name": "demo_posix",
      "inherits": "default",
      "configurePreset": "demo_posix"
    },
    {
      "name": "demo_mingw",
      "inherits": "default",
      "configurePreset": "demo_mingw"
    },
    {
      "name": "demo_pic32mz",
      "inherits": "default",
      "configurePreset": "demo_pic32mz"
    }
  ]
}
